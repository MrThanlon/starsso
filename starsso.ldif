# StarStudio SSO Extra schema
#
#
# Any starstudioer in LDAP should be with organizationalPerson and applicationSystemUser
# , and be identified with email. Other personal information should be filled in attributes
# of organizationalPerson.
# Example:

# OID 前缀表：
#   8.1.2.3.1121212.1 attributetype
#   8.1.2.3.1121212.2 objectclass

dn: cn=starstudio,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: starstudio
# 权限角色 / 权限字
olcAttributeTypes: ( 8.1.2.3.1121212.1.1 NAME 'permissionRoleName'
    DESC 'permission role name to access spcified system'
    EQUALITY caseIgnoreMatch
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{512} )
# 学号
olcAttributeTypes: ( 8.1.2.3.1121212.1.2 NAME 'studentNumber'
    DESC 'student identifier'
    EQUALITY caseIgnoreMatch
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{64} )
# 学制
olcAttributeTypes: ( 8.1.2.3.1121212.1.9 NAME 'schoolLengthType'
    DESC 'type of schooling length'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
# 微信 ID
olcAttributeTypes: ( 8.1.2.3.1121212.1.3 NAME 'wxID'
    DESC 'WeChat ID'
    EQUALITY caseIgnoreMatch
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{512} )
# QQ
olcAttributeTypes: ( 8.1.2.3.1121212.1.4 NAME 'qq'
    DESC 'Tencent QQ number'
    EQUALITY caseIgnoreMatch
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{128} )
# 姓名
olcAttributeTypes: ( 8.1.2.3.1121212.1.5 NAME 'fullName'
    DESC 'full name of member'
    EQUALITY caseIgnoreMatch
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{128} )
# SSO 用户注册时间
olcAttributeTypes: ( 8.1.2.3.1121212.1.6 NAME 'registerTimestamp'
    DESC 'Timestamp of entering SSO system'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
# SSO 注册年份
olcAttributeTypes: ( 8.1.2.3.1121212.1.7 NAME 'registerYear'
    DESC 'Year of entering SSO system'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
# SSO 注册月份
olcAttributeTypes: ( 8.1.2.3.1121212.1.8 NAME 'registerMonth'
    DESC 'Month of entering SSO system'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
# 禁用
olcAttributeTypes: ( 8.1.2.3.1121212.1.10 NAME 'blocked'
    DESC 'indicate whether user is blocked'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.7)
# email
olcObjectClasses: ( 8.1.2.3.1121212.1.11 NAME 'email')
    DESC 'Custom email address'
    SINGLE-VALUE
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.7)
# StarStudio Memeber
olcObjectClasses: ( 8.1.2.3.1121212.2.1 NAME 'starstudioMember'
    DESC 'StarStudio Member'
    SUP organizationalPerson STRUCTURAL
    MUST ( cn $ blocked )
    MAY ( permissionRoleName $ studentNumber $ schoolLengthType $ wxID $ qq $ fullName $ registerTimestamp $ registerYear $ registerMonth $ email) )
